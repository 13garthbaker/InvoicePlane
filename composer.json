{
  "name": "invoiceplane/invoiceplane",
  "version": "2.0.0",
  "description": "InvoicePlane is a self-hosted open source application for managing your invoices, clients and payments",
  "homepage": "https://invoiceplane.com",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.0",
    "codeigniter/framework": "3.1.8",
    "vlucas/phpdotenv": "^2.4",
    "filp/whoops": "^2.1",
    "nesbot/carbon": "^1.27",
    "magefly/Aauth": "*"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.0"
  },
  "repositories":[
    {
      "type": "package",
      "package": {
        "name": "magefly/Aauth",
        "version": "2.5.14",
        "source": {
          "url": "https://github.com/magefly/CodeIgniter-Aauth",
          "type": "git",
          "reference": "2.5-stable"
        }
      }
    }
  ],
  "scripts": {
    "post-update-cmd": [
      "php -r \"copy('vendor/magefly/Aauth/application/libraries/Aauth.php', 'application/libraries/vendor/Aauth.php');\""
    ],
    "post-install-cmd": [
      "php -r \"copy('vendor/magefly/Aauth/application/libraries/Aauth.php', 'application/libraries/vendor/Aauth.php');\""
    ]
  }
}
